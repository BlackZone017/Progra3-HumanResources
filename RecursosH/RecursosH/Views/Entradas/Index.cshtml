@model IEnumerable<RecursosH.Models.Entrada>

@{
    ViewBag.Title = "Index";
}

<h2>Entradas</h2><br />


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.codigoEmpleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Entrada)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.codigoEmpleado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Empleado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Entrada)
        </td>
    </tr>
}

</table>
