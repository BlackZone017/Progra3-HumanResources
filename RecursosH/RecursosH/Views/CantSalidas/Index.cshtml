@model IEnumerable<RecursosH.Models.CantSalida>

@{
    ViewBag.Title = "Index";
}

<div class="container-mio-2">
    <div class="box-mio-2">
        <h1>Salidas Empleados</h1><br />

        <table class="table">
            <tr>
                <th>
                    @Html.DisplayName("ID del Empleado")
                </th>
                <th>
                    @Html.DisplayName("Nombre Empleado")
                </th>
                <th>
                    @Html.DisplayName("Fecha de Salida")
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Empleado)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Fecha_Entrada)
                    </td>
                </tr>
            }

        </table>
    </div>
</div>
